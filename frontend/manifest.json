{
  "manifest_version": 3,
  "name": "Phishing Detection Plugin",
  "version": "1.1.0",
  "icons": { 
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png" 
  },
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["jquery-3.1.1.min.js", "main.js"]
    }
  ],
  "action": {
    "default_popup": "plugin_ui.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png" 
    }
  }
}
